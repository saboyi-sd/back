<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.office.yancao.mapper.PositionLineRuleMapper">
    
    <resultMap id="BaseResultMap" type="com.office.yancao.entity.PositionLineRule">
        <id column="id" property="id" />
        <result column="position_id" property="positionId" />
        <result column="allowed_line" property="allowedLine" />
    </resultMap>
    
    <select id="selectByPositionId" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        SELECT id, position_id, allowed_line
        FROM position_line_rule
        WHERE position_id = #{positionId}
    </select>
</mapper>