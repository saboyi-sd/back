<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.office.yancao.mapper.PositionBaseMapper">
    
    <resultMap id="BaseResultMap" type="com.office.yancao.entity.PositionBase">
        <id column="id" property="id" />
        <result column="position_name" property="positionName" />
        <result column="section" property="section" />
        <result column="page_path" property="pagePath" />
        <result column="line_type" property="lineType" />
    </resultMap>
    
    <select id="selectByPositionName" resultMap="BaseResultMap" parameterType="java.lang.String">
        SELECT id, position_name, section, page_path, line_type
        FROM position_base
        WHERE position_name = #{positionName}
    </select>
</mapper>